<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".myself">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/profile"
        android:text="Proifle Information"
        android:textColor="@color/colorAccent"
        android:textStyle="italic"
        android:textSize="25sp"
        android:layout_marginTop="28dp"
        android:layout_marginLeft="50dp"

        />
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/prfile_info"
        android:layout_marginTop="23dp"
        android:src="@drawable/profile1"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/Delivery"
        android:text="Delivery History and wallet"
        android:textColor="@color/colorAccent"
        android:textStyle="italic"
        android:textSize="25sp"
        android:layout_marginTop="78dp"
        android:layout_marginLeft="50dp"
        />
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/delevery"
        android:layout_marginTop="77dp"
        android:src="@drawable/delivery2"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/call"
        android:text="Call Us"
        android:textColor="@color/colorAccent"
        android:textStyle="italic"
        android:textSize="25sp"
        android:layout_marginTop="128dp"
        android:layout_marginLeft="50dp"
        />
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginTop="125dp"
        android:id="@+id/callus"
        android:src="@drawable/call1"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/request"
        android:text="Request Friend"
        android:textColor="@color/colorAccent"
        android:textStyle="italic"
        android:textSize="25sp"
        android:layout_marginTop="178dp"
        android:layout_marginLeft="50dp"
        />
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginTop="175dp"
        android:id="@+id/requestfriends"
        android:src="@drawable/refer1"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rate"
        android:text="Rate Us"
        android:textColor="@color/colorAccent"
        android:textStyle="italic"
        android:textSize="25sp"
        android:layout_marginTop="228dp"
        android:layout_marginLeft="50dp"
        />
    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginTop="225dp"
        android:id="@+id/rateus"
        android:src="@drawable/rate1"/>


</FrameLayout>